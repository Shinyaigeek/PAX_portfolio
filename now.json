{
  "version": 2,
  "routes": [
    {
      "src": "/",
      "dest": "index.html"
    },
    {
      "src": "/public",
      "dest": "public.html"
    },
    {
      "src": "/gallery",
      "dest": "gallery.html"
    }
  ],
  "functions": {
    "api/getProfile.ts": {
      "memory": 1024,
      "maxDuration": 9
    },
    "api/getWorks.ts": {
      "memory": 1024,
      "maxDuration": 9
    }
  },
  "env": {
    "CONTENTFUL_ACCESS_TOKEN": "@contentful_access_token",
    "CONTENTFUL_SPACE_ID": "@contentful_space_id"
  }
}
